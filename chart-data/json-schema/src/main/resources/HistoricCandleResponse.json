{
  "type": "object",
  "id": "HistoricCandleResponse",
  "properties": {
    "MsgType": {
      "type": "string"
    },
    "ApplVerID": {
      "type": "string",
      "enum": [
        "FIX27",
        "FIX30",
        "FIX40",
        "FIX41",
        "FIX42",
        "FIX43",
        "FIX44",
        "FIX50",
        "FIX50SP1",
        "FIX50SP2"
      ]
    },
    "CstmApplVerID": {
      "type": "string"
    },
    "ReqID": {
      "type": "string"
    },
    "Symbol": {
      "type": "string"
    },
    "Interval": {
      "$ref": "./Interval.json",
      "comment": "for some reason this gets inlined. I do not know why"
    },
    "LastMessage": {
      "type": "boolean"
    },
    "Allowance": {
      "type": "object",
      "id": "Allowance",
      "properties": {
        "RemainingAllowance": {
          "type": "integer"
        },
        "TotalAllowance": {
          "type": "integer"
        },
        "AllowanceExpiry": {
          "type": "integer"
        }
      }
    },
    "SendingTime": {
      "type": "string"
    },
    "CandleData": {
      "comment": "For some reason items after this get ommitted when building the sources. I moved this to the end to stop it from affecting the code",
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "./CandleData.json"
      }
    }
  }
}